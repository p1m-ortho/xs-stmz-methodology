@Article{Wang2009,
  author   = {Wang, Heng-long and Jiang, Zhen-guo and Wang, Zhi-gang and Zhu, Jian and Zhao, Hong-tao},
  title    = {[Analysis of the complication and prevention on treatment of osteoporotic vertebral compressive fractures by percutaneous vertebroplasty (PVP)].},
  journal  = {Zhongguo gu shang = China journal of orthopaedics and traumatology},
  year     = {2009},
  volume   = {22},
